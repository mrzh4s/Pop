{
    "name": "metronic-tailwind-html",
    "version": "9.3.3",
    "description": "Metronic 9 | Tailwind 4 based HTML, JavaScript & React Toolkit for Modern Web Applications",
    "homepage": "https://keenthemes.com/metronic",
    "keywords": [
        "metronic",
        "tailwind",
        "react"
    ],
    "author": "KeenThemes",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build:js": "webpack --mode=development",
        "build:css": "npx @tailwindcss/cli -i ./src/css/styles.css -o ./apps/assets/css/styles.css",
        "build:css:watch": "npx @tailwindcss/cli -i ./src/css/styles.css -o ./apps/assets/css/styles.css --watch",
        "build:prod": "npx @tailwindcss/cli -i ./src/css/styles.css -o ./apps/assets/css/styles.css --minify && npx webpack --env production",
        "build": "npm run build:css && npm run build:js --mode=development",
        "lint": "npx eslint src/core --ext .ts",
        "dev": "concurrently \"npm run dev:css\" \"npm run dev:js\"",
        "dev:css": "npm run build:css:watch",
        "dev:js": "webpack --mode=development --watch",
        "start": "npm run dev"
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@eslint/js": "^9.39.1",
        "@tailwindcss/cli": "^4.1.16",
        "@tailwindcss/forms": "^0.5.10",
        "@types/node": "^20.19.24",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "@typescript-eslint/parser": "^8.46.3",
        "autoprefixer": "^10.4.21",
        "babel-loader": "^10.0.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^9.2.1",
        "copy-webpack-plugin": "^13.0.1",
        "css-loader": "^7.1.2",
        "eslint": "^9.39.1",
        "javascript-obfuscator": "^4.1.1",
        "mini-css-extract-plugin": "^2.9.4",
        "mini-svg-data-uri": "^1.4.4",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-loader": "^8.2.0",
        "postcss-nesting": "^13.0.2",
        "postcss-preset-env": "^10.4.0",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "tailwindcss": "^4.1.16",
        "terser-webpack-plugin": "^5.3.14",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.3",
        "webpack": "^5.102.1",
        "webpack-cli": "^6.0.1",
        "webpack-merge-and-include-globally": "^2.3.4",
        "webpack-obfuscator": "^3.5.1"
    },
    "dependencies": {
        "@fortawesome/fontawesome-pro": "^7.1.0",
        "@keenthemes/ktui": "^1.0.26",
        "@mapbox/mapbox-gl-draw": "^1.5.1",
        "@mapbox/mapbox-gl-geocoder": "^5.1.2",
        "@popperjs/core": "^2.11.8",
        "apexcharts": "^5.3.6",
        "axios": "^1.13.1",
        "clipboard": "^2.0.11",
        "countup.js": "^2.9.0",
        "fslightbox": "^3.7.4",
        "fullcalendar": "^6.1.19",
        "mapbox-gl": "^3.16.0",
        "moment": "^2.30.1",
        "quill": "^2.0.3",
        "quill-image-drop-module": "^1.0.3",
        "quill-image-resize-module": "^3.0.0"
    }
}
